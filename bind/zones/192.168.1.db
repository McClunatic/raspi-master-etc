;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	raspberrypi0.pi.local. root.raspberrypi0.pi.local. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; name servers - NS records
	IN	NS	raspberrypi0.pi.local.

; PTR records
4.1.168.192	IN	PTR	fedorabox.pi.local.	; 192.168.1.4
100.1.168.192	IN	PTR	raspberrypi0.pi.local.	; 192.168.1.100
101.1.168.192	IN	PTR	raspberrypi1.pi.local.	; 192.168.1.101
102.1.168.192	IN	PTR	raspberrypi2.pi.local.	; 192.168.1.102
103.1.168.192	IN	PTR	raspberrypi3.pi.local.	; 192.168.1.103
